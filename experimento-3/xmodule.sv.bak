module xmodule(
  input button, reset, 
  output [13:0] seg
);

  int result = 0;
  
  always @(posedge button, posedge reset)
  
  begin
    if (reset)
      result = 0;
    else if (result == 64)
      result = 0;
	 else
      result = result + 1;
	end
	
  always @(result)
    begin 
      case (result)
        0 : seg = 14'b10000001000000;
        1 : seg = 14'b10000001111001;
        2 : seg = 14'b10000000100100;
        3 : seg = 14'b10000000110000;
        4 : seg = 14'b10000000011001;
        5 : seg = 14'b10000000010010;
        6 : seg = 14'b10000000000010;
        7 : seg = 14'b10000001111000;
        8 : seg = 14'b10000000000000;
        9 : seg = 14'b10000000010000;
        10 : seg = 14'b11110011000000;
        11 : seg = 14'b11110011111001;
        12 : seg = 14'b11110010100100;
        13 : seg = 14'b11110010110000;
        14 : seg = 14'b11110010011001;
        15 : seg = 14'b11110010010010;
        16 : seg = 14'b11110010000010;
        17 : seg = 14'b11110011111000;
        18 : seg = 14'b11110010000000;
        19 : seg = 14'b11110010010000;
        20 : seg = 14'b01001001000000;
        21 : seg = 14'b01001001111001;
        22 : seg = 14'b01001000100100;
        23 : seg = 14'b01001000110000;
        24 : seg = 14'b01001000011001;
        25 : seg = 14'b01001000010010;
        26 : seg = 14'b01001000000010;
        27 : seg = 14'b01001001111000;
        28 : seg = 14'b01001000000000;
        29 : seg = 14'b01001000010000;	
        30 : seg = 14'b01100001000000;
        31 : seg = 14'b01100001111001;
        32 : seg = 14'b01100000100100;
        33 : seg = 14'b01100000110000;
        34 : seg = 14'b01100000011001;
        35 : seg = 14'b01100000010010;
        36 : seg = 14'b01100000000010;
        37 : seg = 14'b01100001111000;
        38 : seg = 14'b01100000000000;
        39 : seg = 14'b01100000010000;
        40 : seg = 14'b01100001000000;
        41 : seg = 14'b00110011111001;
        42 : seg = 14'b00110010100100;
        43 : seg = 14'b00110010110000;
        44 : seg = 14'b00110010011001;
        45 : seg = 14'b00110010010010;
        46 : seg = 14'b00110010000010;
        47 : seg = 14'b00110011111000;
        48 : seg = 14'b00110010000000;
        49 : seg = 14'b00110010010000;	
        50 : seg = 14'b00100101000000;
        51 : seg = 14'b00100101111001;
        52 : seg = 14'b00100100100100;
        53 : seg = 14'b00100100110000;
        54 : seg = 14'b00100100011001;
        55 : seg = 14'b00100100010010;
        56 : seg = 14'b00100100000010;
        57 : seg = 14'b00100101111000;
        58 : seg = 14'b00100100000000;
        59 : seg = 14'b00100100010000;
        60 : seg = 14'b00000101000000;
        61 : seg = 14'b00000101111001;
        62 : seg = 14'b00000100100100;
        63 : seg = 14'b00000100110000;
        default : seg = 14'b01111110111111; 
      endcase
   end
endmodule 